{
  "manifest_version": 2,
  "name": "ChatGPT MathML / LaTeX Copy",
  "version": "2.3",
  "description": "Copy MathML and LaTeX from math elements",
  "permissions": ["activeTab", "contextMenus"],
  "web_accessible_resources": ["*.svg"],
  "icons": {
      "48": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["content.js"]
    }
  ]
}